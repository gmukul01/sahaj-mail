{
	"name": "sahaj-mail",
	"version": "1.0.0",
	"description": "A very simple mail app",
	"main": "index.js",
	"keywords": [
		"mail",
		"inbox"
	],
	"author": "Mukul Bansal",
	"license": "ISC",
	"scripts": {
		"precommit": "lint-staged",
		"start": "run-p start:server start:web",
		"start:web": "rimraf ./build && webpack-dev-server --mode development",
		"start:server": "nodemon ./fakeServer/server.js",
		"build": "rimraf ./build && webpack -p"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"prettier --write",
			"git add"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"eslint": "^5.13.0",
		"eslint-config-prettier": "^4.0.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.3.1",
		"json-server": "^0.14.2",
		"jsonwebtoken": "^8.4.0",
		"lint-staged": "^8.1.3",
		"nodemon": "^1.18.10",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4",
		"react-hot-loader": "^4.6.5",
		"rimraf": "^2.6.3",
		"webpack": "^4.29.3",
		"webpack-cli": "^3.2.3",
		"webpack-dev-server": "^3.1.14"
	},
	"dependencies": {
		"react": "^16.8.1",
		"react-dom": "^16.8.1"
	}
}
