		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.1.0",
		"babel-loader": "^8.0.5",
		"babel-polyfill": "^6.26.0",
		"cors": "^2.8.5",
		"css-loader": "^2.1.0",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.9.1",
		"eslint": "^5.6.0",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-flowtype": "^3.4.1",
