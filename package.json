{
	"name": "sahaj-mail",
	"version": "1.0.0",
	"description": "A very simple mail app",
	"main": "index.js",
	"keywords": [
		"mail",
		"inbox"
	],
	"author": "Mukul Bansal",
	"license": "ISC",
	"scripts": {
		"precommit": "lint-staged",
		"start": "run-p start:server start:web",
		"start:web": "rimraf ./build && webpack-dev-server --mode development",
		"start:server": "nodemon ./fakeServer/server.js",
		"build": "rimraf ./build && webpack --mode production",
		"lint": "eslint ./",
		"test": "jest --config=config/jest.config.js --maxWorkers=5"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"prettier --write",
			"git add"
		]
	},
